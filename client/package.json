{
    "name": "reaact-template-ready",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:ssr": "node server/index.js",
        "build": "vite build",
        "build:ssr": "vite build && vite build --ssr src/entry-server.jsx --outDir dist/server",
        "build:analyze": "vite build --mode analyze",
        "buildsite": "react-scripts build",
        "postbuild": "node scripts/generate-sitemap.js",
        "serve:ssr": "cross-env NODE_ENV=production node server/index.js",
        "sitemap:generate": "node scripts/generate-sitemap.js",
        "sitemap:validate": "node -e \"import('./scripts/generate-sitemap.js').then(({validateSitemap}) => validateSitemap('./public/sitemap.xml'))\"",
        "sitemap:update": "npm run sitemap:generate && npm run sitemap:validate",
        "lint": "eslint .",
        "preview": "vite preview"
    },
    "reactSnap": {
        "inlineCss": true,
        "puppeteerArgs": [
            "--no-sandbox",
            "--disable-setuid-sandbox"
        ]
    },
    "dependencies": {
        "@hookform/resolvers": "^5.2.0",
        "@microsoft/clarity": "^1.0.0",
        "@radix-ui/react-accordion": "^1.2.11",
        "@radix-ui/react-alert-dialog": "^1.1.14",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-navigation-menu": "^1.2.13",
        "@radix-ui/react-radio-group": "^1.3.8",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.2.6",
        "@radix-ui/react-tooltip": "^1.2.7",
        "@tailwindcss/vite": "^4.1.11",
        "axios": "^1.11.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "embla-carousel-react": "^8.6.0",
        "express": "^5.1.0",
        "lucide-react": "^0.532.0",
        "motion": "^12.23.15",
        "next-themes": "^0.4.6",
        "node-cron": "^4.2.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-easy-crop": "^5.5.3",
        "react-hook-form": "^7.61.1",
        "react-router-dom": "^7.7.1",
        "sonner": "^2.0.3",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.11",
        "vite-plugin-beasties": "^0.3.5",
        "xlsx": "^0.18.5",
        "zod": "^4.0.10"
    },
    "devDependencies": {
        "@eslint/js": "^9.25.0",
        "@types/node": "^24.1.0",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@vitejs/plugin-react": "^4.4.1",
        "cross-env": "^10.1.0",
        "eslint": "^9.25.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "node-fetch": "^3.3.2",
        "shadcn": "^3.3.1",
        "sitemap": "^8.0.0",
        "terser": "^5.44.0",
        "tw-animate-css": "^1.3.6",
        "vite": "^6.3.5"
    }
}
